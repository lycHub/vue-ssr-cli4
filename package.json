{
  "name": "my-ssr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:client": "vue-cli-service serve",
    "dev:server": "nodemon server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build:client": "vue-cli-service build",
    "build:server": "cross-env NODE_ENV=production WEBPACK_TARGET=node vue-cli-service build",
    "build": "concurrently \"npm run build:client\" \"npm run build:server\"",
    "start": "cross-env NODE_ENV=production PORT=3800 node server/server.js"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "core-js": "^3.4.4",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-mount": "^4.0.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.2",
    "lib-flexible": "^0.3.2",
    "vue": "^2.6.10",
    "vue-meta": "^2.3.1",
    "vue-router": "^3.1.3",
    "vue-server-renderer": "^2.6.11",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-router": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "concurrently": "^5.0.2",
    "cross-env": "^6.0.3",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "less-plugin-functions": "^1.0.0",
    "memory-fs": "^0.5.0",
    "nodemon": "^2.0.2",
    "vue-template-compiler": "^2.6.10",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
